            <error_message/>
            <gui_type>
                <integer/>
            </gui_type>
        </element>

        <element name="iscsiWriteBytes" parameter_type="output" data_type="decimal" gui_basic="1" range_type="0" units="B/s">
            <control_options>size:8</control_options>
            <default_value/>
            <short_description>iSCSI Write Bytes/sec</short_description>
